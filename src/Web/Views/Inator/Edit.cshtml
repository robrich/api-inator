@model Inator

@{
    ViewBag.Title = Model.InatorId > 0 ? "Edit " + Model.InatorId : "Add new Inator";
}

<a asp-controller="Inator" asp-action="Index"><i class="fa fa-chevron-left"></i> Back to Index</a>

<h2>@ViewBag.Title</h2>

<form asp-controller="Inator" asp-action="Edit" method="post" role="form">
    <div asp-validation-summary="ValidationSummary.All" class="text-danger"></div>
    <label asp-for="Subdomain" class="ccontrol-label"></label><br/>
    <input asp-for="Subdomain" class="form-control" />
    <span asp-validation-for="Subdomain" class="text-danger"></span><br/>
    <button type="submit" class="btn btn-default">Save</button>
</form>
